{
    "name": "moataz/schema-object-manager",
    "description": "Manage database views, procedures, functions and triggers without migrations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Moataz Ahmed"
        }
    ],
    "autoload": {
        "psr-4": {
            "Moataz\\SchemaObjectManager\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "bensampo/laravel-enum": "^6.12",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moataz\\SchemaObjectManager\\SchemaServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}